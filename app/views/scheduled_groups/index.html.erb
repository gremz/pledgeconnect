<h1>Listing scheduled_groups</h1>

<table>
  <thead>
    <tr>
      <th>Schedule</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @scheduled_groups.each do |scheduled_group| %>
      <tr>
        <td><%= scheduled_group.schedule_id %></td>
        <td><%= link_to 'Show', scheduled_group %></td>
        <td><%= link_to 'Edit', edit_scheduled_group_path(scheduled_group) %></td>
        <td><%= link_to 'Destroy', scheduled_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

